let daemonLaunched = async () => {
	return new Promise((resolve) => {
		let catcher = _call('catcherCreate', 'launchdStatus', (a) => {
			if(a.value === 'ru.poppy.opendirectoryd') {
				_call('catcherDestroy', catcher);
				resolve(a.event === 'launched');
			}
		});

		_call('throw', undefined, 'launchdLoad', 'ru.poppy.opendirectoryd');
	});
}

_environment.pamAuthentificate = async (user) => {
	return await daemonLaunched();
}